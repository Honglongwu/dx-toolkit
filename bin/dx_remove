#!/usr/bin/env python

import json
from optparse import OptionParser
import dxpy

parser = OptionParser(usage="%prog [options] id", description="Remove a DNAnexus object from the current project")
(opts, args) = parser.parse_args()

if len(args) != 1:
    parser.print_help()
    parser.error("Incorrect number of arguments")

id = args[0]

print json.dumps(dxpy.dx_remove(id))
