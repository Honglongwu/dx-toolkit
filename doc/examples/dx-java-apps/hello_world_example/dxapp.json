{
  "name": "hello_world_app",
  "title": "Hello, world!",
  "summary": "The obligatory Hello, world app!",
  "description": "This app returns a hash: '{\"greeting\": \"Hello, world!\"}' or Hello, Name, if a name is specified in input",
  "dxapi": "1.0.0",
  "folder": "/programs/hello_world",
  "inputSpec": [
    {"optional": true, "name": "name", "class": "string"}
  ],
  "outputSpec": [
    {"name": "greeting", "class": "string"}
  ],
  "runSpec": {
    "code": "export CLASSPATH=/:/lib:/opt/dnanexus/java/lib/*:$CLASSPATH; java DXHelloWorld",
    "interpreter": "bash",
    "execDepends": [
      {"name": "openjdk-6-jre"}
    ]
  },
  "version": "0.0.1",
  "resources": []
}
